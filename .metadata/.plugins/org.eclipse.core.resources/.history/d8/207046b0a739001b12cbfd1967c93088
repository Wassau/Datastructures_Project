from src import *

class Registro():
    def _init__(self,id:int,satOxi:int,fCardi:int,hora:str,fecha:str):
        self.id = id
        self.satOxi = satOxi
        self.fCardi = fCardi
        self.hora = hora
        self.fecha = fecha 

class User():
    def __init__(self,id : int, username : str,password:str,name : str ,fecha:str,region:str,registro:Registros):
        self.id = id
        self.username = username
        self.password = password
        self.name = name
        self.fecha = fecha
        self.region = region
        self.registro = registro

    def __str__(self):
        return self.name
    
class Registros():
    def __init__(self, listaRegistros : ArrayList):
        self.ListaRegistros = listaRegistros
        self.PromedioOx = 0
        self.PromedioFCar = 0
        
        
us = User('Pero')
print(us.__str__())